{% extends 'base.html.twig' %}

{% block title %}UsuarioExchange{% endblock %}

{% block body %}
<div class="container">
    <h1>Exchange</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Exchange</th>
                <td>{{ usuario_exchange.exchange }}</td>
            </tr>
            <tr>
                <th>Cuenta</th>
                <td>{{ usuario_exchange.cuenta }}</td>
            </tr>
            <tr>
                <th>Clave</th>
                <td>{{ usuario_exchange.clave }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-secondary" href="{{ path('usuario_exchange_index') }}">Volver</a>

    <a class="btn btn-primary" href="{{ path('usuario_exchange_edit', {'id': usuario_exchange.id}) }}">Editar</a>

    {{ include('usuario_exchange/_delete_form.html.twig') }}
</div>
{% endblock %}
